{
    "name": "urbanspaces_backend_app",
    "version": "1.0.0",
    "private": true,
    "description": "Urbanspaces app backend",
    "main": "src/index.ts",
    "scripts": {
        "build": "rm -rf ./dist && yarn install && npx tsc && cp -r ./src/utils/templates ./dist/utils/templates",
        "start:prod": "DOTENV_CONFIG_PATH='.env.production' node -r dotenv/config -r ./tsconfig-paths-bootstrap.js ./dist/index.js",
        "start:staging": "DOTENV_CONFIG_PATH='.env.staging' node -r dotenv/config -r ./tsconfig-paths-bootstrap.js ./dist/index.js",
        "dev": "DOTENV_CONFIG_PATH='.env.local' node -r dotenv/config -r tsconfig-paths/register -r ts-node/register ./src/index.ts",
        "dev:debug": "DOTENV_CONFIG_PATH='.env.local' node --inspect=5258 -r dotenv/config -r tsconfig-paths/register -r ts-node/register ./src/index.ts",
        "dev:watch": "nodemon"
    },
    "keywords": [],
    "author": "Ahman Emmanuel<ahmanemmanuel@gmail.com>",
    "license": "ISC",
    "dependencies": {
        "@hokify/agenda": "^6.3.0",
        "@types/fluent-ffmpeg": "^2.1.21",
        "algoliasearch": "^4.15.0",
        "aws-sdk": "^2.1341.0",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "cross-fetch": "^3.1.5",
        "dotenv": "^16.0.3",
        "express": "^4.17.1",
        "express-fileupload": "^1.4.0",
        "fluent-ffmpeg": "^2.1.2",
        "helmet": "^6.0.1",
        "joi": "^17.7.0",
        "jsonwebtoken": "^9.0.0",
        "moment": "^2.26.0",
        "mongoose": "^6.10.4",
        "morgan": "^1.10.0",
        "postmark": "^3.0.15",
        "pusher": "^5.1.2",
        "randomstring": "^1.2.3",
        "stripe": "^11.7.0",
        "tsconfig-paths": "^4.1.2",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/express": "^4.17.17",
        "@types/express-fileupload": "^1.4.1",
        "@types/express-serve-static-core": "4.17.17",
        "@types/jsonwebtoken": "^9.0.1",
        "@types/node": "^18.11.18",
        "@types/uuid": "^9.0.0",
        "@typescript-eslint/eslint-plugin": "^5.48.2",
        "@typescript-eslint/parser": "^5.48.2",
        "eslint": "^8.32.0",
        "eslint-config-google": "^0.14.0",
        "nodemon": "^2.0.4",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4"
    },
    "engines": {
        "node": "^16.10.0",
        "npm": "^7.24.0"
    }
}
